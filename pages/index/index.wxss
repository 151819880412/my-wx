.indexContainer{
  display: flex;
  flex-direction: column;   /*主侧交换*/
  align-items: center;
  background: #8ed145;
  height: 100%;
}
.indexImg{
  width: 200rpx;
  height: 200rpx;
  border-radius: 100rpx;
  margin: 100rpx 0;
}
.btn{
  width: 200rpx;
  height: 200rpx;
  margin: 100rpx 0;
  border-radius: 100rpx;
  font-size: 24rpx;
  line-height: 200rpx;
  background:pink;
}
.indexName{
  font-size: 36rpx;
  font-weight: bold;
  margin: 100rpx 0;
}
.goStudy {
  width: 220rpx;
  height: 80rpx;
  border: 1rpx solid #eee;
  font-size: 28rpx;
  text-align: center;
  line-height: 80rpx;
  border-radius: 10rpx;
}
